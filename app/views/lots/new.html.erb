<div class="box">
  <h1>Create a stock</h1>
  <%= simple_form_for(@lot) do |f| %>
    <%= f.association :product %>
    <%= f.association :supplier %>
    <%= f.input :expiration_date, as: :string, :label=>"Expiration date", required: false %>
    <%= f.input :cost %>
    <%= f.input :quantity %>
    <%= f.input :photo, as: :file %>
    <%= f.button :submit %>
  <% end %>
</div>
